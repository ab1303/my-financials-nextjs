{
  "name": "my-financials",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:turbo": "next build --turbo",
    "build:webpack": "next build",
    "build:styles": "npx tailwindcss -i ./src/styles/global.css -o ./dist/css/flowbite.css",
    "dev": "next dev --turbo",
    "dev:webpack": "next dev",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "start": "next start",
    "mcp:setup": "npx tsx scripts/mcp-setup.ts",
    "mcp:start": "docker-compose up -d postgres mcp-postgres",
    "mcp:stop": "docker-compose stop mcp-postgres",
    "mcp:restart": "npx tsx scripts/mcp-docker-run.ts restart",
    "mcp:logs": "docker-compose logs -f mcp-postgres",
    "mcp:test": "curl -X GET http://localhost:3001/health || echo 'Testing MCP connection...'",
    "mcp:pull": "docker pull mcp/postgres:latest",
    "db:mcp": "npm run mcp:start && sleep 5 && npm run mcp:test",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@floating-ui/react": "^0.26.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@headlessui/react": "^1.7.16",
    "@hookform/resolvers": "^3.9.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@tanstack/react-query": "^5.62.2",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/google.maps": "^3.58.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "date-fns": "^4.1.0",
    "flowbite": "^1.8.1",
    "flowbite-react": "^0.6.0",
    "immer": "^10.1.1",
    "jsonwebtoken": "^9.0.2",
    "next": "15.4.5",
    "next-auth": "^4.24.10",
    "react": "^18.3.1",
    "react-day-picker": "^8.8.2",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "react-icons": "^5.4.0",
    "react-number-format": "^5.4.2",
    "react-phone-input-2": "^2.15.1",
    "react-select": "^5.8.2",
    "react-toastify": "^10.0.6",
    "superjson": "2.2.1",
    "typescript": "^5.8.3",
    "use-debounce": "^10.0.4",
    "use-places-autocomplete": "^4.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@next/codemod": "^15.4.5",
    "@prisma/client": "^6.13.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.13.0",
    "tailwindcss": "^3.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=10",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "ct3aMetadata": {
    "initVersion": "6.8.0"
  }
}
