{
  "name": "my-financials",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:styles": "npx tailwindcss -i ./src/styles/global.css -o ./dist/css/flowbite.css",
    "dev": "next dev",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "postinstall": "prisma generate",
    "start": "next start",
    "mcp:setup": "npx tsx scripts/mcp-setup.ts",
    "mcp:start": "docker-compose up -d postgres mcp-postgres",
    "mcp:stop": "docker-compose stop mcp-postgres",
    "mcp:restart": "npx tsx scripts/mcp-docker-run.ts restart",
    "mcp:logs": "docker-compose logs -f mcp-postgres",
    "mcp:test": "curl -X GET http://localhost:3001/health || echo 'Testing MCP connection...'",
    "mcp:pull": "docker pull mcp/postgres:latest",
    "db:mcp": "npm run mcp:start && sleep 5 && npm run mcp:test",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@floating-ui/react": "^0.25.4",
    "@googlemaps/js-api-loader": "^1.16.2",
    "@headlessui/react": "^1.7.16",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/google.maps": "^3.53.6",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "flowbite": "^1.8.1",
    "flowbite-react": "^0.6.0",
    "immer": "^10.0.3",
    "jsonwebtoken": "^8.5.1",
    "next": "14.2.30",
    "next-auth": "^4.23.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.8.2",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.45.4",
    "react-icons": "^4.10.1",
    "react-number-format": "^5.3.1",
    "react-phone-input-2": "^2.15.1",
    "react-select": "^5.7.4",
    "react-toastify": "^9.1.3",
    "superjson": "1.9.1",
    "typescript": "^5.8.3",
    "use-debounce": "^9.0.4",
    "use-places-autocomplete": "^4.0.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@next/codemod": "^14.2.30",
    "@prisma/client": "^6.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.27",
    "@types/react-dom": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-config-next": "14.2.30",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^6.13.0",
    "tailwindcss": "^3.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=10",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "ct3aMetadata": {
    "initVersion": "6.8.0"
  }
}
